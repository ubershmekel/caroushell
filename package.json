{
  "name": "caroushell",
  "version": "0.1.16",
  "description": "Terminal carousel that suggests commands from history, config, and AI.",
  "type": "commonjs",
  "main": "dist/main.js",
  "bin": {
    "caroushell": "dist/main.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/main.ts",
    "build": "tsc -p tsconfig.release.json",
    "prepare": "npm run build",
    "start": "node dist/main.js",
    "test:generate": "tsx src/test-generate.ts",
    "lint": "eslint . --ext .ts",
    "release": "tsx scripts/release.ts"
  },
  "keywords": [
    "cli",
    "shell",
    "history",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubershmekel/caroushell.git"
  },
  "bugs": {
    "url": "https://github.com/ubershmekel/caroushell/issues"
  },
  "homepage": "https://github.com/ubershmekel/caroushell#readme",
  "author": "Ubershmekel",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/shell-quote": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "toml": "^3.0.0"
  }
}
